import { ChangeDetectorRef, PipeTransform, OnDestroy } from '@angular/core';
import { IntlService } from '../intl.service';
/**
 * @author Abdel El-medny
 *
 * Shows dates based on the Intl API
 *
 * @see https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/DateTimeFormat
 */
export declare class IntlDatePipe implements PipeTransform, OnDestroy {
    private changeDetectorRef;
    private $internationalisation;
    private lastOutput;
    private lastInput;
    private stop$;
    constructor(changeDetectorRef: ChangeDetectorRef, $internationalisation: IntlService);
    transform(value: Date | string, opts?: Intl.DateTimeFormatOptions): string;
    ngOnDestroy(): void;
}
